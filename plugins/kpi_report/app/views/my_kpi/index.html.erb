    function reset_row(btn) {
        edittable($(btn).parent().parent())
        let btn_save = $($(btn).parent().find('.thaydoi'))
        let btn_reset = $($(btn).parent().find('.reset-row'))
        disable_btn(btn_save, btn_reset)
    }

    function enable_btn(btn_save, btn_reset) {
        $(btn_save).prop("disabled", false)
        $(btn_reset).prop("disabled", false)
        $(btn_save).removeClass("btn-secondary")
        $(btn_reset).removeClass("btn-secondary")
        $(btn_save).addClass("btn-primary")
        $(btn_reset).addClass("btn-danger")
    }

    function disable_btn(btn_save, btn_reset) {
        $(btn_save).prop("disabled", true)
        $(btn_reset).prop("disabled", true)
        $(btn_save).addClass("btn-secondary")
        $(btn_reset).addClass("btn-secondary")
        $(btn_save).removeClass("btn-primary")
        $(btn_reset).removeClass("btn-danger")
    }

    function savetable(btn) {

        $(btn).parent().parent()[0].cells.forEach(element => {
            content = $(element).find('a')
            content.editable('destroy')
        })
    }

    $('#huyModal').on('show.bs.modal', function (event) {
        let button = $(event.relatedTarget) // Button that triggered the modal
        let id = button.data('whatever') // Extract info from data-* attributes
        let modal = $(this)
        let btnhuy = modal.find('.modal-footer #modal-huy')
        btnhuy.click(function () {
            $('#modal-form').validate();
            if ($('#modal-form').valid()) {
                let url = 'http://' + $(location).attr('host') + '/issues/' + id + '.json?key=<%=User.current.api_key%>';
                let data = {
                    "issue": {
                        "status_id": 35,
                        "custom_field_values": {
                            "47": modal.find('.modal-body textarea').val()
                        }
                    }
                }
                $.ajax({
                    type: 'PUT',
                    url: url,
                    contentType: 'application/json',
                    data: JSON.stringify(data),
                }).done(function () {
                    $('#close-huy').click();
                    $($($('#' + id + ' td').first()).find('a').eq(2)).text('4. Hủy (Cancel!!)')
                    $($($('#' + id + ' td').first()).find('a').eq(0)).css("background-color", "#b22222");
                    $($('#' + id + ' td').last()).empty();
                    $($('#' + id + ' td').last()).append('<button style="width: 100px" type="button" onclick="duthao(' + id + ')" class="btn btn-info">Dự thảo</button>')
                }).fail(function (msg) {
                }).always(function (msg) {
                    console.log('ALWAYS');
                });
            }
        });
    })


    function your_onchange_handler() {
        this.form.submit()
