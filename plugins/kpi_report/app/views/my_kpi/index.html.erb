    function duthao(id) {
        let url = 'http://' + $(location).attr('host') + '/issues/' + id + '.json?key=<%=User.current.api_key%>';
        let data = {
            "issue": {
                "status_id": 29,
            }
        }
        $.ajax({
            type: 'PUT',
            url: url,
            contentType: 'application/json',
            data: JSON.stringify(data),
        }).done(function () {
            $($($('#' + id + ' td').first()).find('a').eq(2)).text('0. Dự thảo (Draft)')
            $($($('#' + id + ' td').first()).find('a').eq(0)).css("background-color", "#f1c40f");
            $($('#' + id + ' td').last()).empty();
            $($('#' + id + ' td').last()).append('<button style="width: 100px" type="button" class="btn btn-danger" data-toggle="modal" data-target="#huyModal" data-whatever="' + id + '">Hủy KPI </button>')
        }).fail(function (msg) {
            alert('FAIL')
        }).always(function (msg) {
            console.log('ALWAYS');
        });
    }

